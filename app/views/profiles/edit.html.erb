<%= simple_form_for :profile, method: :patch do |f| %>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
     <%= f.password_field :password, value: :password %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

